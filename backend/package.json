{
    "name": "nodeRT",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "npm run build:live",
        "build": "tsc -p .",
        "build:live": "npx nodemon --watch 'src/**/*.ts' --exec npm run watch",
        "watch": "npx ts-node src/server.ts",
        "start:prod": "nodemon ./dist/server.js",
        "prod": "npm run build && npm run start",
        "prettier:base": "prettier --parser typescript --single-quote",
        "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
        "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/cors": "^2.8.4",
        "@types/express": "^4.16.0",
        "@types/mongoose": "^5.3.8",
        "@types/multer": "^1.3.7",
        "@types/node": "^10.12.18",
        "@types/passport": "^1.0.0",
        "@types/passport-jwt": "^3.0.1",
        "@types/passport-local": "^1.0.33",
        "@types/socket.io": "^2.1.2",
        "nodemon": "^1.18.9",
        "prettier": "^1.16.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.1",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "^3.2.4"
    },
    "dependencies": {
        "bcrypt": "^3.0.3",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.4.5",
        "multer": "^1.4.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "socket.io": "^2.2.0"
    }
}
